[
  {
    "name": "Errores de UI o visibilidad",
    "messageRegex": ".*expected.*to be visible.*",
    "matchedStatuses": ["failed"]
  },
  {
    "name": "Problemas de red o backend",
    "messageRegex": ".*ECONNREFUSED.*|.*NetworkError.*|.*timeout.*",
    "matchedStatuses": ["failed", "broken"]
  },
  {
    "name": "Elementos no encontrados",
    "messageRegex": ".*Timed out retrying.*|.*expected .* to exist.*",
    "matchedStatuses": ["failed"]
  },
  {
    "name": "Tests omitidos manualmente",
    "matchedStatuses": ["skipped"]
  },
  {
    "name": "Ejecuci√≥n correcta",
    "matchedStatuses": ["passed"]
  }
